generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  name       String  @id @unique
  duelist    Boolean @default(false)
  controller Boolean @default(false)
  initiator  Boolean @default(false)
  sentinel   Boolean @default(false)
  path       String  @unique
}

model Map {
  name       String  @id @unique
  inRotation Boolean @default(false)
  path       String  @unique
}

model Strategy {
    name    String
    duelist Boolean @default(false)
    controller  Boolean @default(false)
    initiator   Boolean @default(false)
    sentinel    Boolean @default(false)
    map     String
    explanation String
    path    String  @unique

    @@id([name, duelist, controller, initiator, sentinel, map])
}